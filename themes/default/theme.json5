{
    font: {
        font_size: 13,
        line_height: 18,
    },
    palette: {
        foreground: '#000000',
        background: '#ffffff',
        selected_text_color: '#ffffff',
        selected_text_background: '#308cc6',
    },
    text_input: {
        min_padding: {
            x: 1,
            y: 0,
        },
        preferred_padding: {
            x: 5,
            y: 4,
        },
        min_aspect_ratio: 2,
        preferred_aspect_ratio: 10,
        font: {
            font_size: null,
            line_height: null,
        },
        variants: [
            {
                if: 'always',
                then: {
                    border: {
                        color: '#c8c8c8',
                        width: 1,
                        radius: 2,
                    },
                    background: null,
                    text_color: null,
                    selected_text_color: null,
                    selected_text_background: null,
                },
            },
            {
                if: 'Focused',
                then: {
                    border: {
                        color: '#6464ff',
                        width: null,
                        radius: null,
                    },
                    background: null,
                    text_color: null,
                    selected_text_color: null,
                    selected_text_background: null,
                },
            },
            {
                if: 'MouseOver',
                then: {
                    border: {
                        color: '#ff0000',
                        width: null,
                        radius: null,
                    },
                    background: null,
                    text_color: null,
                    selected_text_color: null,
                    selected_text_background: null,
                },
            },
            {
                if: 'Focused and MouseOver',
                then: {
                    border: {
                        color: '#00ff00',
                        width: null,
                        radius: null,
                    },
                    background: null,
                    text_color: null,
                    selected_text_color: null,
                    selected_text_background: null,
                },
            },
        ],
    },
    button: {
        min_padding: {
            x: 1,
            y: 0,
        },
        preferred_padding: {
            x: 5,
            y: 5,
        },
        font: {
            font_size: null,
            line_height: null,
        },
        variants: [
            {
                if: 'always',
                then: {
                    border: {
                        color: '#c4c4c4',
                        width: 1,
                        radius: 2,
                    },
                    background: {
                        type: 'LinearGradient',
                        start: {
                            x: 0,
                            y: 0,
                        },
                        end: {
                            x: 0,
                            y: 1,
                        },
                        stops: [
                            {
                                position: 0,
                                color: '#fefefe',
                            },
                            {
                                position: 1,
                                color: '#eeeeee',
                            },
                        ],
                        mode: 'Pad',
                    },
                    text_color: null,
                },
            },
            {
                if: 'not Enabled',
                then: {
                    border: {
                        color: null,
                        width: null,
                        radius: null,
                    },
                    background: null,
                    text_color: '#bfbfbf',
                },
            },
            {
                if: 'MouseOver',
                then: {
                    border: {
                        color: null,
                        width: null,
                        radius: null,
                    },
                    background: {
                        type: 'LinearGradient',
                        start: {
                            x: 0,
                            y: 0,
                        },
                        end: {
                            x: 0,
                            y: 1,
                        },
                        stops: [
                            {
                                position: 1,
                                color: '#fefefe',
                            },
                            {
                                position: 1,
                                color: '#f7f7f7',
                            },
                        ],
                        mode: 'Pad',
                    },
                    text_color: null,
                },
            },
            {
                if: 'Pressed',
                then: {
                    border: {
                        color: null,
                        width: null,
                        radius: null,
                    },
                    background: {
                        type: 'Solid',
                        color: '#dbdbdb',
                    },
                    text_color: null,
                },
            },
            {
                if: 'Focused',
                then: {
                    border: {
                        color: '#26709e',
                        width: null,
                        radius: null,
                    },
                    background: null,
                    text_color: null,
                },
            },
        ],
    },
}
